import com.mitrais.blog.reporsitory.BlogRepository;

@RunWith(SpringRunner.class)
@DataJpaTest
public class BlogRepositoryTest {
	@Autowired
    private TestEntityManager entityManager;
	
	@Autowired
	private BlogRepository blogRepository;
}
